{
  "name": "threeflows",
  "description": "Practice spaces for teacher preparation programs",
  "version": "0.1.0",
  "main": "bundle.js",
  "scripts": {
    "build": "cp *.html build && NODE_ENV=production browserify -t [babelify] -g [envify --NODE_ENV 'production'] src/main.jsx | uglifyjs -c -m > build/bundle.js",
    "build-dev": "cp *.html build && browserify -t [babelify] src/main.jsx -o build/bundle.js",
    "watch": "npm run build-dev && watchify -t [babelify] -p livereactload src/main.jsx -o build/bundle.js",
    "flow-quiet": "flow; test $? -eq 0 -o $? -eq 2",
    "lint-quiet": "eslint --fix --ext jsx --ext js -c .eslintrc src; exit 0",
    "lint": "eslint --ext jsx --ext js -c .eslintrc src",
    "mocha-watch": "npm run mocha -- --watch",
    "mocha": "mocha --check-leaks --require test/setup.js --compilers jsx:babel-register",
    "test": "npm run lint && flow && npm run mocha",
    "storybook": "start-storybook -p 9001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mit-teaching-systems-lab/threeflows.git"
  },
  "author": "krob@mit.edu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mit-teaching-systems-lab/threeflows/issues"
  },
  "homepage": "https://github.com/mit-teaching-systems-lab/threeflows#readme",
  "dependencies": {
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "clipboard": "^1.6.1",
    "envify": "^4.0.0",
    "jsdom": "^9.8.0",
    "lodash": "^4.13.1",
    "material-ui": "^0.16.3",
    "msr": "^1.3.4",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.2",
    "react-media": "^1.3.2",
    "react-mini-router": "mit-teaching-systems-lab/react-mini-router",
    "react-motion": "^0.4.4",
    "react-tap-event-plugin": "^2.0.0",
    "react-youtube": "^7.0.1",
    "rollbar-browser": "^1.9.3",
    "superagent": "^2.0.0",
    "superagent-promise": "^1.1.0",
    "uglifyify": "^3.0.4",
    "velocity-animate": "^1.2.3",
    "velocity-react": "^1.1.5"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.2.3",
    "babel-eslint": "^6.0.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "eslint": "^2.13.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.30.0",
    "livereactload": "^2.2.3",
    "mocha": "^3.0.2",
    "react-proxy": "^1.1.8",
    "sinon": "^1.17.5",
    "watchify": "^3.7.0"
  }
}
